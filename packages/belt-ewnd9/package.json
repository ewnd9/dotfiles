{
  "name": "belt-ewnd9",
  "version": "0.0.1",
  "private": true,
  "belt": {
    "commands": {
      "ewnd9:init": {
        "script": "commands/init",
        "description": "init project"
      },
      "ewnd9:provision": {
        "script": "commands/provision",
        "description": "provision/sync dotfiles"
      },
      "ewnd9:dotfiles-docs": {
        "script": "commands/dotfiles-docs",
        "description": "generate docs"
      },
      "ewnd9:remove-submodules": {
        "script": "commands/remove-submodules",
        "description": "remove-submodules"
      },
      "ewnd9:open-editor": {
        "script": "commands/open-editor",
        "description": "open editor in 2nd or 4th tag"
      },
      "ewnd9:incantation": {
        "script": "commands/incantation",
        "description": "regenerate snippets"
      },
      "ewnd9:file-case": {
        "script": "commands/file-case",
        "description": "rename file to certain case (kebab ('-'), snake ('_'), camel ('cC'))"
      },
      "ewnd9:flash-drive": {
        "script": "commands/flash-drive",
        "description": "mount usb"
      },
      "ewnd9:lint-shell": {
        "script": "commands/lint-shell",
        "description": "lint dotfiles shell scripts"
      },
      "ewnd9:licensify": {
        "script": "commands/licensify",
        "description": "add license to README and package.json"
      },
      "ewnd9:install-deps": {
        "script": "commands/install-deps",
        "description": "parse js sources and extract deps"
      },
      "ewnd9:env": {
        "script": "commands/env",
        "description": "stringify .env"
      }
    }
  },
  "dependencies": {
    "@belt/tools": "0.0.0-local",
    "babel-core": "^6.26.3",
    "builtin-modules": "^3.0.0",
    "chalk": "^2.3.0",
    "dedent": "^0.7.0",
    "dotenv-stringify": "^1.0.0",
    "git-url-parse": "^11.1.2",
    "globby": "^8.0.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.13",
    "make-dir": "^3.0.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "parse-github-repo-url": "^1.4.1",
    "prompts": "^2.1.0",
    "qs": "^6.7.0",
    "season": "^6.0.2"
  }
}
