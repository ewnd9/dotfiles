{
  "name": "@belt/cli",
  "version": "0.0.0-local",
  "description": "Global CLI scripts registry",
  "private": true,
  "main": "cli.js",
  "bin": {
    "belt": "cli.js"
  },
  "preferGlobal": true,
  "author": "ewnd9 <ewndnine@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/ewnd9/belt/tree/master/packages/belt-cli",
  "belt": {
    "commands": {
      "ewnd9:incantation": {
        "script": "commands/incantation",
        "description": "regenerate snippets"
      },
      "ewnd9:init": {
        "script": "commands/init",
        "description": "init project"
      },
      "ewnd9:provision": {
        "script": "commands/provision",
        "description": "provision/sync dotfiles"
      },
      "ewnd9:flash-drive": {
        "script": "commands/flash-drive",
        "description": "mount usb"
      },
      "ewnd9:install-deps": {
        "script": "commands/install-deps",
        "description": "parse js sources and extract deps"
      },
      "ewnd9:licensify": {
        "script": "commands/licensify",
        "description": "add license to README and package.json"
      },
      "ewnd9:lint-shell": {
        "script": "commands/lint-shell",
        "description": "lint dotfiles shell scripts"
      },
      "repo:open": {
        "script": "commands/repo-open",
        "description": "open repo in a browser"
      }
    }
  },
  "dependencies": {
    "any-shell-escape": "^0.1.1",
    "chalk": "^2.4.1",
    "execa": "^9.6.0",
    "git-url-parse": "^16.1.0",
    "globby": "^14.1.0",
    "lodash.groupby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "make-dir": "^1.3.0",
    "minimist": "^1.2.0",
    "opn": "^6.0.0",
    "prompts": "^0.1.11"
  }
}
