// Place your settings in this file to overwrite the default settings
{
  "update.mode": "manual",
  "editor.renderWhitespace": "boundary",
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 13,
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.parameterHints.enabled": false,
  "editor.semanticHighlighting.enabled": false,
  "[markdown]": {
    "editor.parameterHints.enabled": true,
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "javascript.suggestionActions.enabled": false,
  "editor.tabCompletion": "onlySnippets",
  "editor.formatOnPaste": false,
  "editor.snippetSuggestions": "top",
  "editor.lineNumbers": "relative",
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "native",
  "explorer.openEditors.visible": 0,
  "workbench.panel.defaultLocation": "right",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "vs-minimal",
  "workbench.colorCustomizations": {
    // https://github.com/Microsoft/vscode/issues/178#issuecomment-343095492
    "gitDecoration.ignoredResourceForeground": "#555555",
    "gitDecoration.modifiedResourceForeground": "#ff982d",
    "gitDecoration.untrackedResourceForeground": "#2bda77",
    "sideBar.background": "#303030",
    "sideBar.foreground": "#aaaaaa"
  },
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": true,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "vim.leader": "<space>",
  "vim.startInInsertMode": false,
  "vim.useSystemClipboard": true,
  // https://github.com/VSCodeVim/Vim/issues/2144
  "vim.handleKeys": {
    "<C-d>": true
  },
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "S-Tab"
      ],
      "after": [
        "<",
        "g",
        "v"
      ]
    },
    {
      "before": [
        "Tab"
      ],
      "after": [
        ">",
        "g",
        "v"
      ]
    }
  ],
  "git.decorations.enabled": true,
  "keyboard.dispatch": "keyCode",
  "extensions.ignoreRecommendations": true,
  // "extensions.autoUpdate": false, // https://github.com/VSCodeVim/Vim/issues/2322#issuecomment-359737038
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "**/.git": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/tmp": true,
    "**/coverage": true,
    "**/.nyc_output/**": true
  },
  "explorer.confirmDragAndDrop": false,
  "prettier.singleQuote": true,
  "prettier.printWidth": 80,
  "problems.decorations.enabled": false,
  "workbench.statusBar.feedback.visible": false,
  // // https://github.com/Microsoft/TypeScript/issues/6209#issuecomment-310578734
  // // disable opening *.d.ts instead of node_modules/<package>
  // "typescript.disableAutomaticTypeAcquisition": true,
  "flow.useNPMPackagedFlow": true,
  "gitlens.keymap": "none",
  "markdown-toc.depthFrom": 2,
  "files.associations": {
    "*.css": "postcss"
  },
  "workbench.statusBar.visible": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "breadcrumbs.enabled": true,
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "gitlens.historyExplorer.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.tsdk": "node_modules/typescript/lib",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.enable": false,
  "go.useLanguageServer": true,
  "terminal.integrated.tabs.location": "left",
  "debug.javascript.autoAttachFilter": "disabled",
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "continue.manuallyRunningServer": true,
  "diffEditor.codeLens": true,
  "git.openRepositoryInParentFolders": "never",
  "python.analysis.extraPaths": [
    "/Users/dudkiniv/.vscode/extensions/continue.continue-0.6.15-darwin-arm64"
  ],
  "python.autoComplete.extraPaths": [
    "/Users/dudkiniv/.vscode/extensions/continue.continue-0.6.15-darwin-arm64"
  ],
  "window.zoomLevel": 1,
  "workbench.activityBar.location": "hidden",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "editor.stickyScroll.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "workbench.colorTheme": "Atom One Dark",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "gitlab.duoCodeSuggestions.enabledSupportedLanguages": {
    "typescript": false,
    "typescriptreact": false,
    "javascript": false,
    "javascriptreact": false,
    "c": false,
    "cpp": false,
    "csharp": false,
    "go": false,
    "haml": false,
    "handlebars": false,
    "java": false,
    "kotlin": false,
    "python": false,
    "php": false,
    "ruby": false,
    "rust": false,
    "scala": false,
    "shellscript": false,
    "sql": false,
    "swift": false,
    "svelte": false,
    "terraform": false,
    "terragrunt": false,
    "vue": false
  },
  "yandex-code-assist.delayedRequestTimeoutMs": 200,
  "yandex-code-assist.showErrorNotifications": true,
  "gitlab.duoChat.enabled": false,
  "gitlab.duoCodeSuggestions.enabled": false,
  "gitlab.duo.enabledWithoutGitlabProject": false,
  "playwright.reuseBrowser": false
}
