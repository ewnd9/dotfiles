'.source.js':
  'Log':
    'prefix': 'log'
    'body': 'console.log($1)'
  'Then':
    'prefix': 'then'
    'body': 'then(data => $1)'
  'Catch':
    'prefix': 'catch'
    'body': 'catch(err => console.log(err.stack || err))'
  'Promise':
    'prefix': 'thencatch'
    'body': """
      then(data => console.log(data))
      .catch(err => console.log(err.stack || err));$1
    """
  'Angular':
    'prefix': 'angular'
    'body': """
      export default {
        scope: {},
        controller: ($scope) => {
          $1
        }
      };
    """
  'React':
    'prefix': 'react'
    'body': """
      import React from 'react';

      export default React.createClass({
        render() {
          return (
            <div>
              $1
            </div>
          );
        }
      });
    """
  'Debugga':
    'prefix': 'debugga'
    'body': '(()=>{debugger})() || $1'
  'Log Chain':
    'prefix': 'logchain'
    'body': 'then(_ => { console.log(_); return _ })'
  'Use Strict':
    'prefix': 'us'
    'body': '\'use strict\';'
  'CommonJS modern require':
    'prefix': 'req'
    'body': 'const ${1:module} = require(\'${1:module}\');'
  'require fs':
    'prefix': 'reqfs'
    'body': 'const fs = require(\'fs\');'
  'import fs':
    'prefix': 'impfs'
    'body': 'import fs from \'fs\';'
  'read file':
    'prefix': 'readfile'
    'body': 'const data = fs.readFileSync($1, \'utf-8\');$2'
  'stringify':
    'prefix': 'str'
    'body': 'JSON.stringify($1, null, 2);$2'
  'export':
    'prefix': 'ex'
    'body': 'export const $1;'
  'connect':
    'prefix': 'connect'
    'body': 'connect(mapStateToProps, mapDispatchToProps)($1)'
  'mapState':
    'prefix': 'mapState'
    'body': """
      const mapStateToProps = $1;
      const mapDispatchToProps = { $2 };
    """
  'redux':
    'prefix': 'redux'
    'body': """
      export const $1_REQUEST = '$1_REQUEST';
      export const $1_SUCCESS = '$1_SUCCESS';
      export const $1_FAILURE = '$1_FAILURE';
    """
  'dispatch':
    'prefix': 'dispatch'
    'body': """
      dispatch({ type: $1_REQUEST });

      return
        .then(data => {
          dispatch({ type: $1_SUCCESS });
        }, err => {
          dispatch({ type: $1_FAILURE, err });
        });
    """
