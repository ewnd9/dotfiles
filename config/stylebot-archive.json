{
    "github.com": {
        "_enabled": true,
        "_rules": {
            "#readme": {
                "margin": "0 auto",
                "width": "800px"
            },
            ".alert": {
                "display": "none"
            },
            ".github-jobs-promotion": {
                "display": "none"
            },
            ".mail-status.unread": {
                "display": "none"
            },
            ".toolbar-commenting": {
                "display": "none "
            },
            "body": {
                "font-size": "13px ",
                "line-height": "1.4 "
            },
            "body, .markdown-body, .intgrs-page .main-content, .blog-content, .migration-wrapper, .migrate-owners-wrapper, .setup-wrapper, .showcase-page-title, .tooltipped::after": {
                "font-family": "Helvetica, arial, nimbussansl, liberationsans, freesans, clean, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol' "
            }
        }
    },
    "vk.com": {
        "_enabled": true,
        "_rules": {
            ".replies_wrap": {
                "display": "none"
            },
            "body": {
                "background-color": "#fafafa"
            }
        }
    },
    "www.kinopoisk.ru": {
        "_enabled": true,
        "_rules": {
            ".criticsRating.criticsRatingDouble": {
                "display": "none"
            },
            ".rating": {
                "display": "none"
            },
            "[name=\"voting_film_form\"]": {
                "display": "none"
            }
        }
    }
}
